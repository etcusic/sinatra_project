ARRAY.map.with_index do |player, i|
    if i >= 0 || i <= 9
        player << "QB"
    elsif i >= 10 || i <= 19
        player << "K"
    elsif i >= 20 || i <= 38
        player << "TE"
    elsif i >= 39 || i < = 66
        player << "RB"
    else
        player << "WR"
    end
end

